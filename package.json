{
  "name": "ngx-source",
  "version": "13.0.2",
  "author": "Sajad Mehrabi",
  "description": "Angular resource loader",
  "license": "MIT",
  "scripts": {
    "build": "ng build:lib",
    "build:lib": "ng build ngx-source",
    "deploy": "npm run build:lib && npm publish --access=public dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "ng": "ng",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "start": "ng serve",
    "test": "ng test"
  },
  "keywords": [
    "ngx",
    "ng",
    "nga",
    "avan",
    "angular",
    "soure loader",
    "resoure loader",
    "library",
    "ngx resoure loader"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mehrabisajad/ngx-source.git"
  },
  "dependencies": {
    "@angular/core": "^13.0.0",
    "rxjs": "^7.4.0",
    "zone.js": "~0.11.4",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular/cli": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/compiler-cli": "13.3.11",
    "@types/jasmine": "5.1.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jasmine-core": "5.1.0",
    "lint-staged": "14.0.0",
    "ng-packagr": "13.3.1",
    "prettier": "3.0.2",
    "prettier-plugin-packagejson": "2.4.5",
    "typescript": "4.5.5"
  }
}
